int[] numbers = { 34, 45, 56, 67, 78, 89, 90, 100, 122, 134 };

            double total=0;
            
            for (int i = 0; i < numbers.Length; i++)
            {
                total=total+numbers[i];
                
            }

            double avarage = total / numbers.Length;

            Console.WriteLine(avarage);
            Console.ReadKey();
